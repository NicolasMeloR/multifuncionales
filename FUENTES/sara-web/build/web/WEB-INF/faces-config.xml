<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
    <managed-bean>
        <managed-bean-name>initBean</managed-bean-name>
        <managed-bean-class>com.davivienda.sara.bean.InitBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>com.davivienda.sara.bean.LoginBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <component>
        <component-type>CSRFTokenInput</component-type>
        <component-class>com.davivienda.sara.corejsf.CSRFTokenInput</component-class>
    </component>
</faces-config>
